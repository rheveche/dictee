{
  "version": 2,
  "builds": [
    {
      "src": "dictee/api/summarize.js",
      "use": "@vercel/node"
    },
    {
      "src": "dictee/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dictee/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "dictee/index.html"
    }
  ]
}
